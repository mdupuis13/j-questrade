      }
    }
  },

  "response": {
    "status": 200,
    "jsonBody": {
      "accounts": [
        {
          "type": "TFSA",
          "number": "99912345",
          "status": "Active",
          "isPrimary": true,
          "isBilling": true,
          "clientAccountType": "Individual"
        },
        {
          "type": "RRSP",
