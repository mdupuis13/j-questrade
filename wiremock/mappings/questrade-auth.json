{
  "request": {
    "method": "GET",
    "urlPath": "/oauth2/token",
    "queryParameters": {
      "grant_type": {
        "equalTo": "refresh_token"
      },
      "refresh_token": {
        "equalTo": "abc123"
      }
    }
  },

  "response": {
    "status": 200,
    "jsonBody": {
      "access_token": "abc-access_token-123",
      "api_server": "https://api03.iq.questrade.com/",
      "expires_in": 1800,
      "refresh_token": "xyz-refresh_token-987",
      "token_type": "Bearer"
    },
    "headers": {
      "Content-Type": "application/json; charset=utf-8"
    }
  }
}
